<%- include('../partials/header') %>
        <table id="list">
                <thead>
                  <tr>
                    <th>Flight</th>
                    <th>Airline<br></th>
                    <th>Departs</th>
                  </tr>
                </thead>
                <tbody>
                  <% flights.forEach(function(f) { %>
                    <tr>
                      <td><%= f.flightNo %></td>
                      <td><%= f.airline %></td>
                      <td><%= f.departs %></td>
                      <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
                    </tr>
                    <% }); %>
                  </tbody>
                </table>
    
</body>
</html>